<form class="pf1 {{cssClass}} flexcol" autocomplete="off">
	<!-- Header -->
	<header>
		<h1 class="charname">
			<input name="name" type="text" value="{{actor.name}}" placeholder="{{localize "ps.partyName"}}" />
		</h1>
	</header>
	<section class="sheet-lower flexrow">
		<!-- Sidebar -->
		<section class="sheet-sidebar sidebar">
			<div class="sheet-profile-img">
				<img class="sheet-profile" src="{{actor.img}}" title="{{actor.name}}" data-edit="img" />
			</div>
		</section>
		<!-- Body -->
	</section>
</form>